    "#Tokenization, breaking up the tweets into single words\n",
    "\n",
    "tokenized_tweet = train['tidy_tweet'].apply(lambda x: x.split())\n",
    "tokenized_tweet"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "metadata": {},
   "outputs": [],
   "source": [
    "#Stemming, converting each tokenized word into its root form and tense\n",
    "#Reference: https://www.datacamp.com/community/tutorials/stemming-lemmatization-python\n",
    "\n",
    "from nltk.stem.porter import *\n",
    "stemmer = PorterStemmer()\n",
    "\n",
    "tokenized_tweet = tokenized_tweet.apply(lambda x: [stemmer.stem(i) for i in x])"
   ]
  },
